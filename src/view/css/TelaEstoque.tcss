TelaEstoque TextArea {
    height: 5;
    margin: 1;
}

TelaEstoque Input {
    margin: 1;
}

TelaEstoque #container_resultado {
    height: auto;
}

TelaEstoque #h_filtro {
    width: auto;
    min-width: 17%;
    height: auto;
    # dock: right;    
    # margin-top: 5;
    # margin-right: 2;
}


TelaEstoque #h_filtro Select {
   width: auto;
   min-width: 75%;
}

TelaEstoque #h_filtro Select Static{
   width: auto;
}


TelaEstoque #h_filtro #seta{
    background: transparent;
    text-align: center;
    height: 100%;
    min-width: 0;
    width: auto;
    border: black 40%;
    color: black;
    margin: 0;
}


TelaEstoque #h_filtro #seta:hover{
    background: rgba(196, 191, 191, 1);
}

App.dark #h_filtro #seta:hover{
    background: #4a4343ff;
}

App.dark #h_filtro #seta{
    background: #1a1717ff;
    color: #ede6e6ff;
    border: #ede6e6ff 40%;
}

TelaEstoque #container_filtragem {
    width: 100%;
    height: auto;
}

TelaEstoque #container_filtragem Select {
    width: 20;
}


TelaEstoque #container_filtragem Static {
    width: 12;
}

TelaEstoque #segundo {
    align: center middle;
    width: 100%;
    height: auto;
}

TelaEstoque #segundo Input {
    height: auto;
    width: auto;
    min-width: 10%;
}

TelaEstoque #segundo Static {
    margin-top: 2;
}

TelaEstoque #primeiro Static {
    margin-top: 1;
}

TelaEstoque #primeiro Select Static {
    margin-top: 0;
}

TelaEstoque #primeiro {
    align: center middle;
    width: 100%;
    height: auto;
}


TelaEstoque Image {
    width: 30;
    height: 10;
    margin-top: 1;
}

TelaEstoque .imovel Checkbox {
    margin-top:4;
    margin-right: 4;
    margin-left: 2;
}

TelaEstoque .imovel {
    background: rgb(200,200,200);
    margin: 2 2 2 2;
    height: 12;
}

App.dark TelaEstoque .imovel{
    background: #1a1717ff;
}

TelaEstoque .dados0 {
    height: 20;
    margin-left: 2;
}

TelaEstoque .dados2 {
    height: 10;
}

TelaEstoque .stt_bairro {
    text-style: bold;
}

TelaEstoque .stt_ref {
    border: black;
    width: auto;
}

App.dark TelaEstoque .stt_ref{
    border: #ede6e6ff;
}